  "license": "MIT",
  "peerDependencies": {
    "express": "^4.14.0",
    "supertest": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "chai": "^3.5.0",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "mocha": "^3.0.2",
    "nodemon": "^1.10.2",
    "sinon": "^1.17.5",
    "supertest": "^2.0.0"
  }
}
